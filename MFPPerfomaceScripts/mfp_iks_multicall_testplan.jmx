        <BeanShellSampler guiclass="BeanShellSamplerGui" testclass="BeanShellSampler" testname="picking device for client" enabled="false">
          <stringProp name="BeanShellSampler.query">// every engine holds unique devices, every thread handles unique device 

//print(${__machineName});
//print(${__threadNum});
int threadsPerEngine = 20;  //num of threads per engine
int devicesPerThread = 1000; //calculate num of devices per thread
int devicesPerEngine = devicesPerThread * threadsPerEngine; //define how many devices any engine should handle
int tNum = Integer.parseInt(&quot;${__threadNum}&quot;); //retrieve threand number
String mName = &quot;${__machineName}&quot;; //retrieve engine name

int numOfEngines = 10;  //how many injectors are participating in the load 

mlist = new String[numOfEngines];  //point the specific names for your engines

mlist[0] = &quot;cl-perf31&quot;;  // range 5-55k
mlist[1] = &quot;cl-perf32&quot;;  // range 55-105k
mlist[2] = &quot;cl-perf33&quot;;   // range 105-155k
mlist[3] = &quot;cl-perf34&quot;;   // range 155-205
mlist[4] = &quot;cl-perf35&quot;;   // ramge 205-255
mlist[5] = &quot;cl-perf36&quot;;   // range 255-305
mlist[6] = &quot;cl-perf37&quot;;   // range 305-355
mlist[7] = &quot;cl-perf38&quot;;   // range 355-405
mlist[8] = &quot;cl-perf39&quot;;   // range 
mlist[9] = &quot;cl-perf40&quot;;   // range 




int engineNum = 0;

//searching for the engine
for (int i = 0; i &lt; numOfEngines; i++) { 
	if (mName == mlist[i]) {
		engineNum = i;
	}
}

int base = engineNum * devicesPerEngine + (tNum - 1) * devicesPerThread;  //seting base according to the engine and thread		

////assuming thread count starts from 1 (not 0) and sequential (1,2,3,4...numOfThreads)
//Random rand = new Random();
//int  n = rand.nextInt(devicesPerThread) + base;  //randomize value- add to base

int threadEngineIdentifier = (tNum * 1000) + engineNum ;
String varName = &quot;current&quot; + threadEngineIdentifier;

int counter = 2;
String varVal = vars.get(varName);

if (varVal != null) {
	counter = Integer.parseInt(vars.get(varName));	
}

int  n = base + counter;
if (++counter == devicesPerThread) {
	counter = 0;
}

vars.put(&quot;device-id&quot;,Integer.toString(n)); //sending ssoid back
vars.put(varName, Integer.toString(counter));

 </stringProp>
